{
  "compilerOptions": {
    "declaration": false,
    "module": "System",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "outDir": "./scripts/dist",
    "outFile": "./scripts/dist/dnn.identitySwitcher.js",
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "Scripts"
  ],
  "files": [
    "./TypeScript/app/app.ts"
  ],
  "include": [
    "./TypeScript/**/*"
  ]
}
